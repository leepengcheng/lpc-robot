<robot name="test_robot">
    <link name="world"/>
    <link name="link1">
        <visual>
            <origin xyz="0.3 0 0.05" rpy="0 0 0" />
            <geometry>
                <box size="0.6 0.2 .1" />
            </geometry>
        </visual>
    </link>
    <link name="link2">
        <visual>
            <origin xyz="0.3 0 0.05" rpy="0 0 0" />
            <geometry>
                <box size="0.6 0.2 0.1" />
            </geometry>
        </visual>
    </link>
    <link name="link3">
        <visual>
            <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://ur_description/meshes/ur5/collision/wrist1.stl"/>
            </geometry>
        </visual>
    </link>
    <link name="link4">
        <!-- <visual><origin xyz="0.3 0 0.05" rpy="0 0 0" /><geometry><box size="0.6 0.2 .1" /></geometry></visual> -->
        <visual>
            <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://ur_description/meshes/ur5/collision/wrist2.stl"/>
            </geometry>
        </visual>
    </link>
    <joint name="world_joint" type="fixed">
        <parent link="world"/>
        <child link="link1"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>
    <joint name="joint1" type="continuous">
        <parent link="link1"/>
        <child link="link2"/>
        <origin xyz="0.6 0 0.1" rpy="0 0 0" />
        <axis xyz="0 1 0"/>
    </joint>
    <joint name="joint2" type="continuous">
        <parent link="link2"/>
        <child link="link3"/>
        <origin xyz="0.6 0 0.1" rpy="0 0 0" />
        <axis xyz="0 0 1"/>
    </joint>
    <joint name="joint3" type="continuous">
        <parent link="link3"/>
        <child link="link4"/>
        <origin xyz="0.6 0 0.1" rpy="0 0 0" />
        <axis xyz="0 0 1"/>
    </joint>
</robot>